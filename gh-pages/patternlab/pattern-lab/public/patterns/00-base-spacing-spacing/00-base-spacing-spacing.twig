{% for item in items %}
  &lt;div style=&quot;margin: 5px;&quot;&gt;
    &lt;div style=&quot;height: {{ item.value }}; width: {{ item.value }}; outline: solid 1px black; display: inline-block; margin-right: 5px;&quot;&gt;&lt;/div&gt;
    &lt;span&gt;{{ item.value }} : &lt;code&gt;{{ item.name }}&lt;/code&gt;&lt;/span&gt;
  &lt;/div&gt;
{% endfor %}

{% if meta %}
  &lt;p&gt; &lt;small&gt;{{ meta.description }}&lt;/small&gt; &lt;/p&gt;
{% endif %}
